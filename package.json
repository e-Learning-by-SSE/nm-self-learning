{
	"name": "self-learning",
	"version": "0.3.4",
	"license": "MIT",
	"scripts": {
		"start": "nx serve",
		"dev": "node scripts/dev-start.js",
		"build": "nx build --skip-nx-cache",
		"build:affected": "nx affected:build --parallel",
		"test": "env TZ='Europe/Berlin' jest --runInBand",
		"test:affected": "env TZ='Europe/Berlin' nx affected:test --parallel=3 --runInBand",
		"cypress": "cypress open --project=apps/site-e2e",
		"prettier:check": "prettier --check **/*",
		"prettier:write": "prettier --write '{apps,libs}/**/*.{ts,tsx}'",
		"prisma": "prisma",
		"preprisma:seed": "prisma generate",
		"prisma:seed": "prisma db push --accept-data-loss && prisma db seed",
		"start:prod": "cd dist/apps/site && npm start"
	},
	"private": true,
	"prisma": {
		"schema": "libs/data-access/database/prisma/schema.prisma",
		"seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} --project libs/data-access/database/tsconfig.json --require tsconfig-paths/register libs/data-access/database/src/lib/seed.ts"
	},
